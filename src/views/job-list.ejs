<div class="row my-card-row" id="job-list">

    <h1 class="mb-5"><i class="bi bi-list-ol"></i> Available Jobs</h1>
    <% job.forEach((jobobj)=>{%>
            <div class="col-12 col-md-5 col-lg-4 mb-5">
                <div class="my-card " id="custom-border">
                    <h2 class="mb-4 "><i class="bi bi-buildings"></i>
                        <%=jobobj.company%>
                    </h2>
                    <h5><i class="bi bi-person-check"></i><%=jobobj.position%></h5>
                    <p>
                    </p>
                    <h5><i class="bi bi-geo-alt"></i><%=jobobj.location%></h5>
                    <p>
                    </p>
                    <h5><i class="bi bi-laptop"></i><%=jobobj.skills%></h5>
                    <p class="mb-5">
                    </p>
                    <a class="my-btn">More Details</a>
                </div>
            </div>
            <%})%>
                    <p>No jobs available</p>
                    


</div>